{
    "$schema": "https://www.schemastore.org/package.json",
    "private": true,
    "name": "digitalsos-cms",
    "version": "1.0.0",
    "description": "Digital'SOS - Système de gestion sportive avec méthode M2PC",
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "clean": "rm -rf public/css public/js public/mix-manifest.json",
        "fresh": "npm run clean && npm run dev"
    },
    "devDependencies": {
        "@popperjs/core": "^2.11.8",
        "alpinejs": "^3.4.2",
        "autoprefixer": "^10.4.23",
        "axios": "^1.11.0",
        "bootstrap": "^5.3.8",
        "concurrently": "^9.0.1",
        "jquery": "^3.7.1",
        "laravel-mix": "^6.0.49",
        "postcss": "^8.5.6",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.97.2",
        "sass-loader": "^16.0.6"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^7.1.0"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=8.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead",
        "not ie 11"
    ]
}
